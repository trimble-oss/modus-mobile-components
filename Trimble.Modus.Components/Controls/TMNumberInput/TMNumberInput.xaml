<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Trimble.Modus.Components.TMNumberInput"
             xmlns:controls="clr-namespace:Trimble.Modus.Components"
             xmlns:constants="clr-namespace:Trimble.Modus.Components.Constant">
    <controls:TMInput
      x:Name="TMInputControl"
      Keyboard="Numeric"
      LeftIconSource="{x:Static constants:ImageConstants.MinusImage}"
      RightIconSource="{x:Static constants:ImageConstants.PlusImage}"
      TextChanged="OnTextChanged"/>
</ContentView>
